{
  "name": "@examples/base",
  "version": "0.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --inspect dist/index.js",
    "watch": "tsbb watch",
    "build": "tsbb build",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage --bail"
  },
  "author": "kenny wong <wowohoo@qq.com>",
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "!src/**/*.{js,ts,json}",
      "!*.json",
      "!coverage/**/*.json"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/?!(.*)"
    ]
  },
  "dependencies": {
    "typenexus": "0.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsbb": "^4.1.5"
  }
}
